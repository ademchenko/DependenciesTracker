<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
		<Version>2.0.0.1</Version>
		<PackageId>DependenciesTracking</PackageId>
		<Authors>Alexander Demchenko</Authors>
		<Company>Alexander Demchenko</Company>
		<Product>DependenciesTracking</Product>
		<Description>Brings infrastructure for observing property paths changes and automatically updating properties that depend on that paths.</Description>
		<Copyright>©  2022 Alexander Demchenko</Copyright>
		<PackageProjectUrl>https://github.com/ademchenko/DependenciesTracker/</PackageProjectUrl>
		<RepositoryUrl>https://github.com/ademchenko/DependenciesTracker/</RepositoryUrl>
		<AssemblyVersion>2.0.0.1</AssemblyVersion>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>..\DependenciesTracking.snk</AssemblyOriginatorKeyFile>
		
	</PropertyGroup>

	<ItemGroup>
	  <Folder Include="Properties\" />
	</ItemGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="DependenciesTracking.Tests, " />
	</ItemGroup>
</Project>